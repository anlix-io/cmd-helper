{
  "list": [
    {
      "command": "mkdir",
      "args": ["teste"],
      "workingDirectory": ".",
      "expects": [
        {
          "exitWithMessage": "Directory created",
          "expectedStatus": 0
        }
      ]
    },
    {
      "command": "touch",
      "args": ["teste/teste.txt"],
      "workingDirectory": ".",
      "expects": [
        {
          "exitWithMessage": "File created",
          "expectedStatus": 0
        }
      ]
    },
    {
      "command": "ls",
      "args": [],
      "workingDirectory": "teste",
      "expects": [
        {
          "expectedOutput": "teste.txt",
          "exitWithMessage": "File found",
          "expectedStatus": 0
        },
        {
          "exitWithMessage": "Error: No such file or directory",
          "expectedStatus": 1
        }
      ]
    },
    {
      "command": "ls",
      "args": ["-l"],
      "workingDirectory": "teste",
      "expects": [
        {
          "expectedOutput": "-rw-r--r-- 1 root root  0 Dec  1 00:00 teste.txt\n",
          "exitWithMessage": "File found",
          "expectedStatus": 0
        },
        {
          "exitWithMessage": "Error: No such file or directory",
          "expectedStatus": 1
        }
      ]
    },
    {
      "command": "rm",
      "args": ["teste/teste.txt"],
      "workingDirectory": ".",
      "expects": [
        {
          "exitWithMessage": "File removed",
          "expectedStatus": 0
        },
        {
          "exitWithMessage": "Error: No such file or directory",
          "expectedStatus": 1
        }
      ]
    },
    {
      "command": "rm",
      "args": ["-fr", "teste"],
      "workingDirectory": ".",
      "expects": [
        {
          "exitWithMessage": "Directory removed",
          "expectedStatus": 0
        },
        {
          "exitWithMessage": "Error: No such file or directory",
          "expectedStatus": 1
        }
      ]
    }
  ]
}